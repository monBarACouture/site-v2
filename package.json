  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/monBarACouture/site-v2.git"
  },
  "keywords": [
    "metalsmith",
    "foundation"
  ],
  "author": "mBàC",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/monBarACouture/site-v2/issues"
  },
  "homepage": "https://github.com/monBarACouture/site-v2#readme",
  "private": true,
  "devDependencies": {
    "del": "^2.2.2",
    "eslint": "^3.5.0",
    "handlebars": "^4.0.5",
    "gulp": "^3.9.1",
    "gulp-livereload": "^3.8.1",
    "gulp-metalsmith": "^1.0.0",
    "metalsmith": "^2.2.0",
    "metalsmith-layouts": "^1.6.5",
    "metalsmith-markdown": "^0.2.1"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "plugins": [],
    "parserOptions": {
      "ecmaFeatures": {},
      "sourceType": "module"
    },
    "rules": {
      "camelcase": 0,
      "curly": [
        1,
        "multi-line"
      ],
      "strict": [
        2,
        "global"
      ],
      "key-spacing": 0,
      "new-cap": [
        1,
        {
          "newIsCap": true,
          "capIsNew": false
        }
      ],
      "no-multi-spaces": 0,
      "quotes": [
        1,
        "single"
      ],
      "space-infix-ops": 0
    }
  }
}
