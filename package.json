{
  "name": "mbac-site-v2",
  "version": "0.0.0",
  "description": "Version 2 of mon Bar à Couture website.",
  "main": "index.js",
  "scripts": {
    "dev": "$PWD/tools/scripts/dev.sh",
    "dist": "$PWD/tools/scripts/dist.sh",
    "test": "$PWD/tools/scripts/test.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/monBarACouture/site-v2.git"
  },
  "keywords": [
    "metalsmith",
    "foundation"
  ],
  "author": "mBàC",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/monBarACouture/site-v2/issues"
  },
  "homepage": "https://github.com/monBarACouture/site-v2#readme",
  "private": true,
  "devDependencies": {
    "async": "^2.0.1",
    "babel-core": "^6.14.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.14.0",
    "babel-register": "^6.14.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "chai": "^3.5.0",
    "del": "^2.2.2",
    "eslint": "^3.5.0",
    "font-awesome": "^4.6.3",
    "foundation-sites": "^6.2.4-rc2",
    "fs-tools": "^0.2.11",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-cli": "^1.2.2",
    "gulp-htmlmin": "^3.0.0",
    "gulp-if": "^2.0.1",
    "gulp-livereload": "^3.8.1",
    "gulp-metalsmith": "^1.0.0",
    "gulp-sass": "^2.3.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^2.0.0",
    "gulp-util": "^3.0.7",
    "gulp-webserver": "^0.9.1",
    "handlebars": "^4.0.5",
    "jquery": "^2.2.4",
    "lodash": "^4.16.2",
    "metalsmith": "^2.2.0",
    "metalsmith-layouts": "^1.6.5",
    "metalsmith-markdown": "^0.2.1",
    "metalsmith-permalinks": "^0.5.0",
    "mocha": "^3.0.2",
    "object.entries": "^1.0.3",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.7.0"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "browser": true,
      "es6": true,
      "jquery": true,
      "node": true
    },
    "plugins": [],
    "parserOptions": {
      "ecmaFeatures": {},
      "sourceType": "module"
    },
    "rules": {
      "camelcase": 0,
      "curly": [
        1,
        "multi-line"
      ],
      "strict": [
        2,
        "global"
      ],
      "key-spacing": 0,
      "new-cap": [
        1,
        {
          "newIsCap": true,
          "capIsNew": false
        }
      ],
      "no-multi-spaces": 0,
      "quotes": [
        1,
        "single"
      ],
      "space-infix-ops": 0
    }
  },
  "env": {
    "production": {
      "content": {
        "metadata": {
          "siteTitle": "mon Bar à Couture",
          "url": "http://monbaracouture.com"
        }
      }
    },
    "development": {
      "content": {
        "metadata": {
          "siteTitle": "mon Bar à Couture",
          "url": "http://localhost:8000"
        }
      }
    }
  }
}
